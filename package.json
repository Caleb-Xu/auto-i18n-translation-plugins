{
    "name": "i18n_translation_vite",
    "version": "0.0.0",
    "scripts": {
        "dev": "node script/build.js d",
        "preview": "node script/preview.js",
        "build": "node script/build.js",
        "publish:plugin": "cd vitePluginsAutoI18n && pnpm publish",
        "publishWp:plugin": "cd webpackPluginsAutoI18n && pnpm publish",
        "format": "npx prettier . --write",
        "prepare": "husky install"
    },
    "description": "vite auto-translation plugin based on Google api, not intrusive to the source code.",
    "devDependencies": {
        "@inquirer/prompts": "^4.1.0",
        "@types/node": "^20.3.3",
        "@types/shelljs": "^0.8.15",
        "husky": "^8.0.0",
        "prettier": "^3.2.5",
        "pretty-quick": "^4.0.0",
        "shelljs": "^0.8.5"
    },
    "dependencies": {
        "axios": "^1.6.8",
        "crypto-js": "^4.2.0"
    }
}
